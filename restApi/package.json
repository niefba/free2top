{
   "name": "rest-api",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/node": "^16.11.10",
      "nodemon": "^3.1.7",
      "ts-node": "10.9.1",
      "typescript": "^4.5.2"
   },
   "dependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/express": "^5.0.0",
      "@types/jsonwebtoken": "^9.0.7",
      "@types/memory-cache": "^0.2.6",
      "bcrypt": "^5.1.1",
      "dotenv": "^16.4.6",
      "express": "^4.21.1",
      "jsonwebtoken": "^9.0.2",
      "memory-cache": "^0.2.0",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.20"
   },
   "nodemonConfig": {
      "watch": ["src"],
      "ext": "ts,json",
      "ignore": ["src/**/*.spec.ts"],
      "exec": "ts-node ./src/index.ts"
   },
   "scripts": {
      "start": "NODE_PATH=./build node build/index.js",
      "build": "tsc -p .",
      "dev": "nodemon",
      "typeorm": "typeorm-ts-node-commonjs"
   }
}
